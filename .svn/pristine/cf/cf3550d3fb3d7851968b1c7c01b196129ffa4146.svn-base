.batteryScanCode_page {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;

  .scanCode_mian {
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;

    .scanCode_text {
      font-size: 26px;
    }

    .scanCodeBox {
      margin-top: 10Px;
      width: 658px;
      height: 540px;
      position: relative;
      display: flex;
      justify-content: center;
      align-items: center;

      .scanCodeBorder {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
      }

      .cpImg {
        width: 390px;
        height: 418px;
      }
    }
  }

  .scanCode_open {
    margin: 20px 30px;
    height: 200px;
  }
}
